<Window x:Class="ImageProcessingWPF.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:ImageProcessingWPF.Views" 
        xmlns:vm="clr-namespace:ImageProcessingWPF.ViewModels"
        d:DataContext="{d:DesignInstance Type=vm:MainViewModel}"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1600">
    <TabControl>
        <TabItem Header="Images">
            <views:ImageComparator />
        </TabItem>

        <TabItem Header="String">
            <views:StringComparator />
        </TabItem>
    </TabControl>
    
    
</Window>
